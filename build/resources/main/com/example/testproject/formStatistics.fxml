<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"  spacing="20" maxHeight="Infinity">
    <HBox maxWidth="Infinity" alignment="CENTER_RIGHT" spacing="10">
        <Button text=" Weekly Report " styleClass="btnReport" fx:id="btnWeek">
            <graphic>
                <FontIcon iconLiteral="bi-file-earmark-text" iconColor="dimgrey" iconSize="20"/>
            </graphic>
        </Button>
        <Button text=" Monthly Report " styleClass="btnReport" fx:id="btnMonth" >
            <graphic>
                <FontIcon iconLiteral="bi-file-earmark-text" iconColor="dimgrey" iconSize="20"/>
            </graphic>
        </Button>
        <Button text=" Yearly Report " styleClass="btnReport" fx:id="btnYear">
            <graphic>
                <FontIcon iconLiteral="bi-file-earmark-text" iconColor="dimgrey" iconSize="20"/>
            </graphic>
        </Button>
        <Button text=" List Of Services " styleClass="btnReport" fx:id="btnServices">
            <graphic>
                <FontIcon iconLiteral="bi-file-earmark-text" iconColor="dimgrey" iconSize="20"/>
            </graphic>
        </Button>
    </HBox>
    <VBox fx:id="formStatisticsPie" HBox.hgrow="ALWAYS" alignment="CENTER" styleClass="wrap-form-general">
    </VBox>

</VBox>